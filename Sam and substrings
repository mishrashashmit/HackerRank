#define MOD 1000000007

int substrings(char* n) {
    int len = strlen(n);
        long long total = 0;
        long long prev = 0;
                for (int i = 0; i < len; i++) {
                    int digit = n[i] - '0';
                    long long curr = ((prev * 10) % MOD + ((long long)(i + 1) * digit) % MOD) % MOD;
                    total = (total + curr) % MOD;
                    prev = curr;
        }                  
    return (int)total;
}
